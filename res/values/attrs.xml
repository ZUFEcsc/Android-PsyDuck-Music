<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 搜索框 -->
    <declare-styleable name="SearchView">
        <!-- 搜索框的文字 -->
        <attr name="sv_text" format="string"/>
        <!-- 提示文字 -->
        <attr name="sv_hint" format="string"/>
        <!-- 字体大小 -->
        <attr name="sv_textSize" format="dimension"/>
        <!-- 是否隐藏搜索图标 -->
        <attr name="sv_hideImg" format="boolean"/>
    </declare-styleable>
</resources>